/slab
{
	gsave
	currentpoint newpath moveto
	0 1 index rlineto
	1 index 0 rlineto
	0 1 index neg rlineto
	closepath
	pop pop
	
	gsave setgray fill grestore
	gsave 0 setlinewidth stroke grestore
	grestore
} def
/cursor
{
	gsave
	0 -2 2 index mul rmoveto
	.8 6 2 index mul 10 3 index mul slab
	grestore
	pop
} def
/square
{
	gsave
	0 0 2 index mul rmoveto
	1 3 2 index mul 3 3 index mul slab
	grestore
	pop
} def
/perp
{
	gsave
	dup
	S exch fp
	6.6 mul dup rmoveto
	180 rotate
	(\136) show
	grestore
} def
